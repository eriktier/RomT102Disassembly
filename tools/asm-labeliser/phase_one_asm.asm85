7687H  (E6H) ANI 07H    	; Mask all but byte count???
7689H  (CAH) JZ L768FH       ; Optional external controller driver
768CH  (36H) MVI M,00H  	; No bytes to read  indicate Zero bytes transferred???
768EH  (C9H) RET


