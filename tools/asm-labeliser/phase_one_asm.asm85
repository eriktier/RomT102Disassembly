    MVI M,00H
    DCX H
    STC
    RET


; ======================================================
; L4696H: Input routine ENTER handler
; ======================================================
IHENTR:
    POP H
    CALL CRLF      ; Send CRLF to screen or printer
    XRA A
    STAX D
    JMP L7FFAH      ; Different from M100
    RET


; ======================================================
